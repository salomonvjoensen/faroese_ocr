      <TextLine id="eSc_line_367db90e" custom="structure {type:default;}">
        <Coords points="341,1695 341,1712 2160,1716 2162,1695 2162,1656 341,1652 341,1695"/>
        <Baseline points="341,1695 2162,1695"/>
        <TextEquiv>
          <Unicode>Ein dagin um heystið, sum teir vóru á fjalli í Dølun, noyddist Rógvu-Jógvan</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_b5822acf" custom="structure {type:default;}">
        <Coords points="302,1767 302,1784 1922,1784 1922,1767 1922,1730 302,1726 302,1767"/>
        <Baseline points="302,1767 1922,1767"/>
        <TextEquiv>
          <Unicode>heim aftur til hús um kvøldið, tí konan skuldi eiga barn, men hinir</Unicode>
        </TextEquiv>
      </TextLine>
