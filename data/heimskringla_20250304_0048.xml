        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_a9ec1fd6" custom="structure {type:default;}">
        <Coords points="341,1574 2149,1576 2152,1548 2148,1509 1711,1509 1661,1498 1659,1498 1606,1508 1536,1498 1534,1498 1438,1508 561,1500 559,1500 557,1500 555,1500 540,1511 444,1511 368,1500 366,1500 364,1500 339,1509 341,1552"/>
