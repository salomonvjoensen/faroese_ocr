        <Baseline points="300,872 951,872"/>
        <TextEquiv>
          <Unicode>Huldusøgn úr Vestmanna</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_318cf7ee" custom="structure {type:default;}">
        <Coords points="2175,937 302,937 302,980 302,997 2187,997 2187,980 2183,954 2175,937"/>
        <Baseline points="302,980 2187,980"/>
        <TextEquiv>
          <Unicode>Fyrr í tíðini varð deyðseyði ikki pikkaður í Vestmanna bygd í Norðstreymoy í tí</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_a1829bf5" custom="structure {type:default;}">
        <Coords points="302,1052 302,1069 2080,1065 2082,1048 2076,1005 300,1009 302,1052"/>
        <Baseline points="302,1052 2082,1048"/>
        <TextEquiv>
          <Unicode>hagaparti, sum kallast í Dølum, og hildið varð, at ein huldumaður mundi</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_a3f8a24c" custom="structure {type:default;}">
        <Coords points="302,1118 300,1133 370,1145 372,1145 920,1137 1113,1147 1115,1147 1918,1137 1920,1122 1918,1085 302,1081 302,1118"/>
        <Baseline points="302,1118 1920,1122"/>
        <TextEquiv>
          <Unicode>búgva har og ansa eftir deyðseyðanum. Hvørt ár um heystið, tá ið</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_e5f37fed" custom="structure {type:default;}">
        <Coords points="306,1217 2162,1219 2162,1194 2162,1155 304,1147 304,1194"/>
        <Baseline points="304,1194 2162,1194"/>
        <TextEquiv>
          <Unicode>Vestmenningar vóru á fjalli í Dølum til at taka skurð, løgdu teir besta seyðin</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_ec59290a" custom="structure {type:default;}">
