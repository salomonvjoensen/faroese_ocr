        <Baseline points="282,1005 1908,997"/>
        <TextEquiv>
          <Unicode>Niðurskrivað av V. U. Hammershaimb eftir frásøgn av Joen Danielsen í Skálavík</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_b5fde340" custom="structure {type:default;}">
        <Coords points="280,1146 1957,1138 1959,1112 1955,1081 1555,1071 1553,1071 1551,1071 1532,1081 1514,1081 1119,1073 1117,1073 1074,1083 1070,1083 1045,1083 711,1075 709,1075 707,1075 672,1085 666,1087 278,1087 280,1120"/>
        <Baseline points="280,1120 1959,1112"/>
        <TextEquiv>
          <Unicode>Ein prestur átti ein hjall, sum hann goymdi brenni í, men brenniviðurin varð so illa</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_dc1d775b" custom="structure {type:default;}">
        <Coords points="282,1229 2128,1221 2129,1196 2125,1165 1666,1153 1665,1153 1505,1165 1298,1155 1296,1155 1169,1167 781,1157 779,1157 777,1157 738,1169 559,1159 557,1159 555,1159 520,1169 485,1159 483,1159 481,1159 337,1169 296,1159 294,1159 292,1159 290,1159 288,1159 280,1163 282,1204"/>
        <Baseline points="282,1204 2129,1196"/>
        <TextEquiv>
          <Unicode>burturstolin frá honun. Ein sunnudag, sum prestafólkini vóru farin í kirkju og arbeiðskonan</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_e6d6c160" custom="structure {type:default;}">
        <Coords points="281,1308 2168,1306 2170,1278 2166,1244 393,1243 391,1243 280,1252 282,1283"/>
        <Baseline points="282,1283 2170,1278"/>
        <TextEquiv>
          <Unicode>var einsumøll eftir, fór hon í kjallin at taka brenni. Meðan hon var har, sá hon eina lúku ganga</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_7e57787f" custom="structure {type:default;}">
        <Coords points="282,1389 2173,1389 2175,1361 2171,1332 1702,1320 1700,1320 1698,1320 1674,1332 1672,1332 1668,1332 1626,1322 1624,1322 1491,1332 1475,1332 1460,1322 1458,1322 1456,1322 1304,1322 1302,1322 1241,1332 1224,1334 1206,1332 1150,1324 1148,1324 1097,1332 1082,1336 928,1336 924,1332 912,1324 910,1324 908,1324 906,1324 801,1332 752,1336 736,1332 711,1326 709,1326 707,1326 705,1326 686,1332 674,1338 540,1338 532,1332 522,1326 520,1326 518,1326 516,1326 280,1332 282,1369"/>
        <Baseline points="282,1369 2175,1361"/>
        <TextEquiv>
          <Unicode>leysa í gólvinun og ein røvara koma upp ígjøgnun. Hon tók svørðið og høgdi høvdið av honun,</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_5e361ec0" custom="structure {type:default;}">
        <Coords points="284,1475 2138,1469 2140,1443 2134,1412 1618,1412 1524,1402 1522,1402 1460,1412 1431,1402 1429,1402 1427,1402 1306,1412 1271,1402 1269,1402 1267,1402 1208,1414 1165,1404 1163,1404 1162,1404 1115,1414 865,1404 863,1404 744,1416 719,1406 717,1406 364,1406 362,1406 282,1418 284,1451"/>
        <Baseline points="284,1451 2140,1443"/>
        <TextEquiv>
          <Unicode>og síðan drap hon ellivu røvarar, hvønn undan øðrum, sum teir komu upp ígjøgnun lemmin.</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_6b8755d9" custom="structure {type:default;}">
        <Coords points="282,1554 2179,1553 2179,1529 2175,1492 280,1492 282,1535"/>
        <Baseline points="282,1535 2179,1529"/>
        <TextEquiv>
          <Unicode>Men tá ið hin tólvti og síðsti kom, faftaði hon við - hann slapp undan og lovaði, at hann skuldi</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_47c60c79" custom="structure {type:default;}">
