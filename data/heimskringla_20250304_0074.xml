      <TextLine id="eSc_line_2b63efbf" custom="structure {type:default;}">
        <Coords points="308,978 2004,978 2002,954 2000,921 308,917 308,950"/>
        <Baseline points="308,950 2002,954"/>
        <TextEquiv>
