        <Baseline points="329,335 2093,329"/>
        <TextEquiv>
          <Unicode>Tá tók huldan í Húsagjógv til at gleiva og gina so skjótt, at hon nærkaðist hestinun í</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_0ad834c9" custom="structure {type:default;}">
        <Coords points="280,428 280,444 2088,442 2090,422 2086,389 278,385 280,428"/>
        <Baseline points="280,428 2090,422"/>
