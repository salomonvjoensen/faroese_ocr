      <TextLine id="eSc_line_06551acc" custom="structure {type:default;}">
        <Coords points="356,350 356,373 2173,381 2175,356 2173,319 370,309 356,317"/>
        <Baseline points="356,350 2175,356"/>
        <TextEquiv>
          <Unicode>Har sum Barbara sat uppi á mønuni í Múla, kann ikki flag heftast. Tað skal koma av tí,</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_0fedd44a" custom="structure {type:default;}">
        <Coords points="2195,405 2183,399 2181,399 2179,399 1068,403 1017,393 1015,393 703,401 682,391 680,391 678,391 311,399 311,403 311,434 310,455 2199,457 2203,444 2203,409 2201,407 2201,442"/>
        <Baseline points="311,434 2203,444"/>
        <TextEquiv>
          <Unicode>at Barbara lovaði tað í hevnd, at har skuldi altíð leika tað sama óveðrið, sum tá ið hon sat</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_61a528f5" custom="structure {type:default;}">
        <Coords points="308,533 1138,539 1142,524 1140,490 310,483 310,520"/>
        <Baseline points="310,520 1142,524"/>
        <TextEquiv>
